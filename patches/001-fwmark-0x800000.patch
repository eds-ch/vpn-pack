# Tailscale-Version: 1.94.1
# Description: Change subnet route fwmark from 0x40000 to 0x800000 to avoid conflict with UniFi wgclt1
--- a/tsconst/linuxfw.go	2026-02-21 10:13:02.217119588 +0000
+++ b/tsconst/linuxfw.go	2026-02-21 10:13:02.221119582 +0000
@@ -33,8 +33,8 @@
 
 	// Packet is from Tailscale and to a subnet route destination, so
 	// is allowed to be routed through this machine.
-	LinuxSubnetRouteMark    = "0x40000"
-	LinuxSubnetRouteMarkNum = 0x40000
+	LinuxSubnetRouteMark    = "0x800000"
+	LinuxSubnetRouteMarkNum = 0x800000
 
 	// Packet was originated by tailscaled itself, and must not be
 	// routed over the Tailscale network.
--- a/util/linuxfw/linuxfw.go	2026-02-21 10:13:02.224119578 +0000
+++ b/util/linuxfw/linuxfw.go	2026-02-21 10:13:02.227119573 +0000
@@ -91,7 +91,7 @@
 
 // getTailscaleSubnetRouteMark returns the TailscaleSubnetRouteMark in bytes.
 func getTailscaleSubnetRouteMark() []byte {
-	return []byte{0x00, 0x04, 0x00, 0x00}
+	return []byte{0x00, 0x80, 0x00, 0x00}
 }
 
 // checkIPv6ForTest can be set in tests.
